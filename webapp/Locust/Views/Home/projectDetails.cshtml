@Scripts.Render("~/bundles/AdjustUI")
@{
    Layout = null;
}
<div class="right_col" role="main">
    <nav aria-label="breadcrumb">
        <ol class="breadcrumb">
            <li class="breadcrumb-item">
                <a href="#">Home</a>
            </li>
            <li class="breadcrumb-item active" aria-current="page">{{name}}</li>
        </ol>
    </nav>
    <div class="clearfix"></div>
    <div class="row">
        <div class="col-md-12 col-sm-12 col-xs-12">
            <div class="x_panel">
                <div class="x_title">
                    <h2>
                        Project "{{name}}"
                        <small> Details</small>
                    </h2>
                    <ul class="nav navbar-right panel_toolbox" id="demo">
                        <li>
                            <a class="collapse-link" data-toggle="collapse" data-target="#hi" id="collapse"><i class="fa fa-chevron-up"></i></a>
                        </li>
                    </ul>
                    <div class="clearfix"></div>
                </div>
                <script>
                    $(document).ready(function () {
                        $("#hi").on("hide.bs.collapse", function () {
                            $("#demo").html('<li> <a class="collapse-link" data-toggle="collapse" data-target="#hi" id="collapse"><i class="fa fa-chevron-down"></i></a></li>');
                        });
                        $("#hi").on("show.bs.collapse", function () {
                            $("#demo").html('<li> <a class="collapse-link" data-toggle="collapse" data-target="#hi" id="collapse"><i class="fa fa-chevron-up"></i></a></li>');
                        });
                        $('a[data-toggle="tab"]').on('show.bs.tab', function (e) {
                            localStorage.setItem('activeTab', $(e.target).attr('data-target'));
                        });
                        var activeTab = localStorage.getItem('activeTab');
                        if (activeTab) {
                            $('a[data-target="' + activeTab + '"]').tab('show');
                        }
                    });
                </script>
                @* Here start the contern *@
                <div class="x_content collapse in" id="hi" aria-expanded="true">
                    <div class="customContainer">
                        <div>
                            <a class="customTitle">Project ID:</a>
                            <a class="message">{{id}}</a>
                        </div>
                        <div>
                            <a class="customTitle"> Description:</a>
                            <p class="message">{{description}}</p>
                        </div>
                        <div>
                            <a class="customTitle">Status:</a>
                            <span class="label {{class}}">{{status}}</span>
                            @*<span ng-if="status==='Inactive'" class="label label-danger">{{status}}</span>*@
                        </div>
                        <div class="input-group-btn" style="text-align:right">
                            <button type="button" class="btn btn-default" data-toggle="modal" data-target="#modalTagsForm"><i class="fa fa-tags" aria-hidden="true"></i> Tags</button>
                            <button type="button" class="btn btn-default" data-toggle="modal" ng-click="GetGroups()"><i class="fa fa-file-text" aria-hidden="true"></i> Project Report Excel</button>
                            <button type="button" ng-click="goToExecutionGroups(id)" class="btn btn-default" aria-label="Change Log"><i class="fa fa-play" aria-hidden="true"></i> Executions Groups</button>
                            <button type="button" ng-click="goToProjectExecution(id)" class="btn btn-default" aria-label="Change Log"><i class="fa fa-check" aria-hidden="true"></i> Executions Results</button>
                            <button type="button" data-toggle="modal" data-target="#modalUsersForm" ng-click="GetUsers(id)" class="btn btn-default" aria-label="Change Log"><i class="fa fa-users" aria-hidden="true"></i> Users</button>
                            <button type="button" class="btn btn-default" aria-label="Change Log" ui-sref="projectChangeLog({id:id})"><i class="fa fa-history" aria-hidden="true" ui-sref="projectChangeLog({id:id})"></i> Change Log </button>
                            <button type="button" class="btn btn-default" aria-label="Change Log" ui-sref="testEnvironment({projectId:id})"><i class="fa fa-server" aria-hidden="true" ui-sref="testEnvironment({projectId:id})"></i> Test Environments </button>
                            <button type="button" ng-click="traceabilityDetails(id)" class="btn btn-default" aria-label="Change Log"><i class="fa fa-search" aria-hidden="true"></i>Traceability Findings (MER)</button>
                        </div>
                    </div>
                </div>
                <br />
                <div class="row tile_count">
                    <div class="col-md-2 col-sm-4 col-xs-6 tile_stats_count">
                        <span class="count_top">
                            <i class="fa fa-sticky-note-o"></i> Total Requirements
                        </span>
                        <div class="count">{{dashboard.totalRequirements}}</div>
                    </div>
                    <div class="col-md-2 col-sm-4 col-xs-6 tile_stats_count">
                        <span class="count_top"><i class="fa fa-warning"></i> Req. Missing Test Evidence</span>
                        <div class="count">{{dashboard.requirementsMissingTestEvidence}}</div>
                    </div>
                    <div class="col-md-2 col-sm-4 col-xs-6 tile_stats_count">
                        <span class="count_top"><i class="fa fa-users"></i> Users With Access</span>
                        <div class="count">{{users.length}}</div>
                    </div>
                    <div class="col-md-2 col-sm-4 col-xs-6 tile_stats_count">
                        <span class="count_top">
                            <i class="fa fa-server"></i> Test Environments
                        </span>
                        <div class="count">{{TestEnv}}</div>
                    </div>
                    <div class="col-md-2 col-sm-4 col-xs-6 tile_stats_count">
                        <span class="count_top">
                            <i class="fa fa-file-o"></i> Supplemental Test Procedures
                        </span>
                        <div class="count">{{dashboard.totalstp}}</div>
                    </div>
                    <div class="col-md-2 col-sm-4 col-xs-6 tile_stats_count">
                        <span class="count_top"><i class="fa fa-files-o"></i> Test Evidence</span>
                        <br>
                        <span class="count_bottom">
                            <i class="ng-binding">
                                <i class="fa fa-file-o"></i> <strong>TP:</strong>
                            </i> {{dashboard.totalTestProcedures}}
                        </span>
                        <br>
                        <span class="count_bottom"><i class="ng-binding"><i class="fa fa-file-o"></i> <strong>TC:</strong> </i>  {{dashboard.totalTestCases}}</span>
                        <br>
                        <span class="count_bottom"><i class="ng-binding"><i class="fa fa-file-o"></i> <strong>TS:</strong> </i> {{dashboard.totalTestScenarios}}</span>
                        <br>
                    </div>

                </div>

                <div class="col-x_content-12">
                    <div class="tab" role="tabpanel">
                        <!-- Nav tabs -->
                        <ul class="nav nav-tabs" role="tablist">
                            <li role="presentation" class="active">
                                <a data-target="#tab_content1" id="home-tab" role="tab" data-toggle="tab" aria-expanded="true">Requirements</a>
                            </li>
                            <li role="presentation" class="">
                                <a data-target="#tab_content2" role="tab" id="profile-tab" data-toggle="tab" aria-expanded="false">Supplemental Test Procedures</a>
                            </li>
                            <li role="presentation" class="">
                                <a data-target="#tab_content3" role="tab" id="profile-tab" data-toggle="tab" aria-expanded="false">Scripts Groups</a>
                            </li>
                        </ul>
                        <!-- Tab panes -->
                        <div class="tab-content tabs">
                            <div role="tabpanel" class="tab-pane fade active in" id="tab_content1" aria-labelledby="home-tab">
                                <div class="col-md-12 col-sm-12 col-xs-12">
                                    <div class="form-group row">
                                        <div class="col-md-1 col-sm-4 col-xs-4 selectCustom">
                                            <select name="priority" required class="form-control" ng-model="RowsSelectedReq" ng-change="changeValueReq(RowsSelectedReq)"
                                                    ng-options="option as option for option in Options"></select>
                                        </div>
                                        <div class="col-md-3 col-sm-8 col-xs-8">
                                            <div class="input-group">
                                                <input auto-focus ng-model="filterREQ" type="text" class="form-control" id="filter" autocomplete="off" placeholder="Search">
                                                <span class="input-group-btn">
                                                    <button class="btn btn-default filterButton" type="button"><i class="fa fa-search"></i></button>
                                                </span>
                                            </div>
                                        </div>
                                        <div class="col-md-8 col-sm-12 col-xs-12" align="right">
                                            <span class="input-group-btn">
                                                <button style="margin-right:10px" ng-if="CurrentUserRole!='VBP'" class="btn btn-default" type="button" data-placement="top" data-toggle="tooltip" ui-sref="requirementAdd({projectId:id})" data-original-title="Forward" aria-describedby="tooltip802045"><i class="fa fa-plus"></i> New Requirement</button>
                                                <button class="btn btn-default" type="button" data-placement="top" data-toggle="tooltip" ui-sref="requirementActivate({projectId:id})" data-original-title="Forward" aria-describedby="tooltip802045"><i class="fa fa-folder-open"></i> Inactive Requirements</button>
                                            </span>
                                        </div>
                                    </div>
                                </div>

                                <table style="overflow:hidden" class="table table-striped table-bordered dataTable no-footer dtr-inline" role="grid" aria-describedby="datatable-buttons_info">
                                    <thead>
                                        <tr role="row">
                                            <th class="sorting_asc" tabindex="0" aria-controls="datatable-buttons" rowspan="1" colspan="1" style="width: 218px;" aria-sort="ascending" aria-label="Name: activate to sort column descending">Axosoft ID</th>
                                            <th class="sorting_asc" tabindex="0" aria-controls="datatable-buttons" rowspan="1" colspan="1" style="width: 218px;" aria-sort="ascending" aria-label="Name: activate to sort column descending">Number</th>
                                            <th class="sorting_asc" tabindex="0" aria-controls="datatable-buttons" rowspan="1" colspan="1" style="width: 218px;" aria-sort="ascending" aria-label="Name: activate to sort column descending">Requirement</th>
                                            <th class="sorting" tabindex="0" aria-controls="datatable-buttons" rowspan="1" colspan="1" style="width: 167px;" aria-label="Developer Assigned: activate to sort column ascending">Developer Assigned</th>
                                            <th class="sorting" tabindex="0" aria-controls="datatable-buttons" rowspan="1" colspan="1" style="width: 92px;" aria-label="Tester Assigned: activate to sort column ascending">Tester Assigned</th>
                                            <th class="sorting" tabindex="0" aria-controls="datatable-buttons" rowspan="1" colspan="1" style="width: 92px;" aria-label="Actions: activate to sort column ascending">Actions</th>
                                    </thead>
                                    <tbody style="overflow:hidden">
                                        <tr role="row" class="odd" href="" dir-paginate="requirement in filteredArray = (requirements | REQFilter:filterREQ) | itemsPerPage: pageSizeReq" current-page="currentPageREQ" pagination-id="paginationREQ">
                                            <td tabindex="0" class="sorting_1" ng-click="requirementDetails(requirement.Id)">{{requirement.Axosoft_Task_Id}}</td>
                                            <td tabindex="0" class="sorting_1" ng-click="requirementDetails(requirement.Id)">{{requirement.req_number}}</td>
                                            <td tabindex="0" style="overflow:hidden;" class="sorting_1" ng-click="requirementDetails(requirement.Id)">{{requirement.Name}}</td>
                                            <td style="overflow:hidden" ng-click="requirementDetails(requirement.Id)">{{requirement.Developer_Assigned}}</td>
                                            <td style="overflow:hidden" ng-click="requirementDetails(requirement.Id)">{{requirement.Tester_Assigned}}</td>
                                            <td>
                                                <div class="btn-group">
                                                    <button class="btn btn-sm btn-default" type="button" data-placement="top" data-toggle="tooltip" data-original-title="Forward" aria-describedby="tooltip802045" ng-click="requirementDetails(requirement.Id)"><i class="fa fa-info-circle"></i></button>
                                                    <button ng-if="CurrentUserRole!='VBP'" class="btn btn-sm btn-default" type="button" data-placement="top" data-toggle="tooltip" data-original-title="Print" ui-sref="requirementEdit({id:requirement.Id, projectId:id})"><i class="fa fa-pencil"></i></button>
                                                    <button ng-if="CurrentUserRole!='VBP'" data-toggle="modal" data-target="#modalRequirementForm" class="btn btn-sm btn-default" type="button" data-placement="top" data-original-title="Trash" ng-click="reqSelected(requirement)"><i class="fa fa-trash-o"></i></button>
                                                </div>
                                            </td>
                                        </tr>
                                        <tr ng-if="filteredArray.length===0">
                                            <td colspan="6">@Resource.NoDataFound</td>
                                        </tr>
                                    </tbody>
                                </table>
                                <div class="dataTables_info" id="datatable-buttons_info" role="status" aria-live="polite">
                                    {{filteredArray.length}} entries
                                </div>
                                <div class="paginationContainer">
                                    <dir-pagination-controls boundary-links="true" pagination-id="paginationREQ" template-url="Pages/dirPagination.tpl.html"></dir-pagination-controls>
                                </div>
                            </div>
                            <div role="tabpanel" class="tab-pane fade" id="tab_content2" aria-labelledby="profile-tab">
                                <div class="col-md-12 col-sm-12 col-xs-12">
                                    <div class="form-group row">
                                        <div class="col-md-1 col-sm-4 col-xs-4 selectCustom">
                                            <select name="priority" required class="form-control" ng-model="RowsSelectedSupp" ng-change="changeValueSupp(RowsSelectedSupp)"
                                                    ng-options="option as option for option in Options"></select>
                                        </div>
                                        <div class="col-md-3 col-sm-8 col-xs-8">
                                            <div class="input-group">
                                                <input auto-focus ng-model="filterSTP" type="text" class="form-control" id="filter" autocomplete="off" placeholder="Search">
                                                <span class="input-group-btn">
                                                    <button class="btn btn-default filterButton" type="button"><i class="fa fa-search"></i></button>
                                                </span>
                                            </div>
                                        </div>
                                        <div class="col-md-8 col-sm-12 col-xs-12" align="right">
                                            <span class="input-group-btn">
                                                <button style="margin-right:10px" ng-if="CurrentUserRole!='VBP' && CurrentUserRole!='BA'" class="btn btn-default" type="button" data-placement="top" data-toggle="tooltip" ui-sref="suplementalAdd({id: id, projectId:id})" data-original-title="Forward" aria-describedby="tooltip802045"><i class="fa fa-plus"></i> New Supplemental Test Procedure</button>
                                                <button class="btn btn-default" type="button" data-placement="top" data-toggle="tooltip" data-original-title="Forward" ng-click="navigateActivateTestSupplemental()" aria-describedby="tooltip802045"><i class="fa fa-folder-open"></i> Inactive Test Supplementals</button>
                                            </span>
                                        </div>
                                    </div>
                                </div>

                                <table class="table table-striped table-bordered dataTable no-footer dtr-inline" role="grid" aria-describedby="datatable-buttons_info">
                                    <thead>
                                        <tr role="row">
                                            <th class="sorting_asc" tabindex="0" aria-controls="datatable-buttons" rowspan="1" colspan="1" style="width: 218px;" aria-sort="ascending" aria-label="Name: activate to sort column descending">STP Number</th>
                                            <th class="sorting" tabindex="0" aria-controls="datatable-buttons" rowspan="1" colspan="1" style="width: 167px;" aria-label="Title: activate to sort column ascending">Title</th>
                                            <th class="sorting" tabindex="0" aria-controls="datatable-buttons" rowspan="1" colspan="1" style="width: 92px;" aria-label="Creator: activate to sort column ascending">Creator</th>
                                            <th class="sorting" tabindex="0" aria-controls="datatable-buttons" rowspan="1" colspan="1" style="width: 92px;" aria-label="Actions: activate to sort column ascending">Actions</th>
                                    </thead>
                                    <tbody>
                                        <tr role="row" class="odd" href="" dir-paginate="suplemental in filteredArraySTP = (suplementals | STPFilter: filterSTP) | itemsPerPage: pageSizeSupp" current-page="currentPageSTP" pagination-id="paginationSTP">
                                            <td tabindex="0" class="sorting_1" ng-click="suplementalDetails(suplemental.Test_Suplemental_Id)">{{suplemental.stp_number}}</td>
                                            <td tabindex="0" style="overflow:hidden" class="sorting_1" ng-click="suplementalDetails(suplemental.Test_Suplemental_Id)">{{suplemental.Title}}</td>
                                            <td style="overflow:hidden" ng-click="suplementalDetails(suplemental.Test_Suplemental_Id)">{{suplemental.Test_Procedure_Creator}}</td>
                                            <td>
                                                <div class="btn-group">
                                                    <button class="btn btn-sm btn-default" type="button" data-placement="top" data-toggle="tooltip" data-original-title="Forward" aria-describedby="tooltip802045" ng-click="suplementalDetails(suplemental.Test_Suplemental_Id)"><i class="fa fa-info-circle"></i></button>
                                                    <button ng-if="CurrentUserRole!='VBP' && CurrentUserRole!='BA'" class="btn btn-sm btn-default" type="button" data-placement="top" data-toggle="tooltip" data-original-title="Print" ui-sref="suplementalEdit({id:suplemental.Test_Suplemental_Id, projectId:id})"><i class="fa fa-pencil"></i></button>
                                                    <button ng-if="CurrentUserRole!='VBP' && CurrentUserRole!='BA'" data-toggle="modal" data-target="#modalSuplementalForm" class="btn btn-sm btn-default" type="button" data-placement="top" data-original-title="Trash" ng-click="supSelected(suplemental)"><i class="fa fa-trash-o"></i></button>
                                                    <button ng-if="CurrentUserRole!='VBP' && CurrentUserRole!='BA'" class="btn btn-sm btn-default" type="button" ng-click="rowSelected(suplemental,id,'STP')"><i class="fa fa-copy"></i></button>
                                                </div>
                                            </td>
                                        </tr>
                                        <tr ng-if="filteredArraySTP.length===0">
                                            <td colspan="4">@Resource.NoDataFound</td>
                                        </tr>
                                    </tbody>
                                </table>
                                <div class="dataTables_info" id="datatable-buttons_info" role="status" aria-live="polite">
                                    {{filteredArraySTP.length}} entries
                                </div>
                                <div class="paginationContainer">
                                    <dir-pagination-controls boundary-links="true" pagination-id="paginationSTP" template-url="Pages/dirPagination.tpl.html"></dir-pagination-controls>
                                </div>
                            </div>
                            <!--Scripts Groups-->
                            <div role="tabpanel" class="tab-pane fade" id="tab_content3" aria-labelledby="profile-tab">
                                <div class="col-md-12 col-sm-12 col-xs-12">
                                    <div class="form-group row">
                                        <div class="col-md-1 col-sm-4 col-xs-4 selectCustom">
                                            <select name="priority" required class="form-control" ng-model="RowsSelectedSupp" ng-change="changeValueSupp(RowsSelectedSupp)"
                                                    ng-options="option as option for option in Options"></select>
                                        </div>
                                        <div class="col-md-3 col-sm-8 col-xs-8">
                                            <div class="input-group">
                                                <input auto-focus ng-model="filterGroup" type="text" class="form-control" id="filter" autocomplete="off" placeholder="Search">
                                                <span class="input-group-btn">
                                                    <button class="btn btn-default filterButton" type="button"><i class="fa fa-search"></i></button>
                                                </span>
                                            </div>
                                        </div>
                                        <div class="col-md-8 col-sm-12 col-xs-12" align="right">
                                            <span class="input-group-btn">
                                                <button style="margin-right:10px" ng-if="CurrentUserRole!='VBP' && CurrentUserRole!='BA'" class="btn btn-default" type="button" data-placement="top" data-toggle="tooltip" ng-click="NavigateAddScriptGroup()" data-original-title="Forward" aria-describedby="tooltip802045"><i class="fa fa-plus"></i> New Script Group</button>
                                            </span>
                                        </div>
                                    </div>
                                </div>

                                <table class="table table-striped table-bordered dataTable no-footer dtr-inline" role="grid" aria-describedby="datatable-buttons_info">
                                    <thead>
                                        <tr role="row">
                                            <th class="sorting_asc" tabindex="0" aria-controls="datatable-buttons" rowspan="1" colspan="1" style="width: 218px;" aria-sort="ascending" aria-label="Name: activate to sort column descending">Name</th>
                                            <th class="sorting" tabindex="0" aria-controls="datatable-buttons" rowspan="1" colspan="1" style="width: 167px;" aria-label="Title: activate to sort column ascending">Release</th>
                                            <th class="sorting" tabindex="0" aria-controls="datatable-buttons" rowspan="1" colspan="1" style="width: 92px;" aria-label="Creator: activate to sort column ascending">Priority</th>
                                            <th class="sorting" tabindex="0" aria-controls="datatable-buttons" rowspan="1" colspan="1" style="width: 92px;" aria-label="Creator: activate to sort column ascending">Creator</th>
                                            <th class="sorting" tabindex="0" aria-controls="datatable-buttons" rowspan="1" colspan="1" style="width: 92px;" aria-label="Actions: activate to sort column ascending">Creation Date</th>
                                            <th class="sorting" tabindex="0" aria-controls="datatable-buttons" rowspan="1" colspan="1" style="width: 92px;" aria-label="Actions: activate to sort column ascending">Actions</th>
                                    </thead>
                                    <tbody>
                                        <tr role="row" class="odd" href="" dir-paginate="script in ScriptGroupsFiltered = (ScriptGroups | GroupsFilter: filterGroup) | itemsPerPage: pageSizeScript" current-page="currentPageScript" pagination-id="paginationScript">
                                            <td tabindex="0" class="sorting_1">{{script.Name}}</td>
                                            <td tabindex="0" style="overflow:hidden" class="sorting_1">{{script.Release}}</td>
                                            <td style="overflow:hidden">{{script.Priority}}</td>
                                            <td style="overflow:hidden">{{script.Creator}}</td>
                                            <td style="overflow:hidden">{{script.Creation_Date}}</td>
                                            <td>
                                                <div class="btn-group">
                                                    <button class="btn btn-sm btn-default" type="button" data-placement="top" data-toggle="tooltip" data-original-title="Forward" aria-describedby="tooltip802045"><i class="fa fa-info-circle"></i></button>
                                                    <button ng-if="CurrentUserRole!='VBP' && CurrentUserRole!='BA'" class="btn btn-sm btn-default" type="button" data-placement="top" data-toggle="tooltip" data-original-title="Print"><i class="fa fa-pencil"></i></button>
                                                </div>
                                            </td>
                                        </tr>
                                        <tr ng-if="ScriptGroupsFiltered.length===0">
                                            <td colspan="6">@Resource.NoDataFound</td>
                                        </tr>
                                    </tbody>
                                </table>
                                <div class="dataTables_info" id="datatable-buttons_info" role="status" aria-live="polite">
                                    {{ScriptGroups.length}} entries
                                </div>
                                <div class="paginationContainer">
                                    <dir-pagination-controls boundary-links="true" pagination-id="paginationScript" template-url="Pages/dirPagination.tpl.html"></dir-pagination-controls>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<div class="modal fade" id="modalRequirementForm" tabindex="-1" role="dialog" aria-labelledby="myModalLabel"
     aria-hidden="true">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h3>Disable Requirement</h3>
            </div>
            <div class="modal-body">
                <p style="overflow:hidden">Are you sure you want to disable the Requirement: {{rselected.Name}}</p>
            </div>
            <div class="modal-footer">
                <button class="btn btn-danger" ng-click="showModalToDelete(rselected)" data-dismiss="modal">Disable</button>
                <button type="button" class="btn btn-primary" data-dismiss="modal">Close</button>
            </div>
        </div>
    </div>
</div>
<div class="modal fade" id="modalUsersForm" tabindex="-1" role="dialog" aria-labelledby="myModalLabel"
     aria-hidden="true">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h3>Users Assigned ({{users.length}})</h3>
            </div>
            <div class="modal-body" style="max-height:700px; width:100%; overflow-y:scroll">
                <div>
                    <div ng-repeat="user in users" class="media block-update-card">
                        <a class="pull-left" href="">
                            <img class="media-object update-card-MDimentions" ng-src="{{user.PhotoUrl}}">
                        </a>
                        <div class="media-body update-card-body">
                            <h4 style="margin-top:15px;" class="media-heading">
                                {{user.UserName}} (
                                <strong>{{user.Role}}</strong>)
                            </h4>
                            <h4 class="media-heading">{{user.FirstName}} {{user.LastName}}</h4>
                            <h4 class="media-heading">{{user.Email}}</h4>
                        </div>
                    </div>
                </div>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-primary" data-dismiss="modal">Close</button>
            </div>
        </div>
    </div>
</div>
<div class="modal fade" id="modalTagsForm" tabindex="-1" role="dialog" aria-labelledby="myModalLabel"
     aria-hidden="true">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h3>{{action}} Tags</h3>
            </div>

            <div class="x_content">
                <form name="form" autocomplete="off" id="form" novalidate class="form-horizontal form-label-left" ng-submit="AddTag(form)" align="right" style="border:0">
                    <div class="form-group col-sm-12 col-md-12">
                        <label for="tagName" class="control-label col-md-4">
                            Tag Name <span class="required">*</span>
                        </label>
                        <div class="col-md-8 col-sm-8 col-xs-8">
                            <input ng-model="tagName" type="text" name="tagName" id="tagName" ng-maxlength="19" autofocus required class="form-control col-sm-8 col-md-2">
                        </div>
                    </div>

                    <div class="form-group">
                        <div class="col-md-4 col-sm-4 col-xs-4 col-md-offset-4" style="text-align:right">
                            <span class="input-group-btn">
                                <button class="btn btn-default" ng-disabled="btnSave" type="submit" style="margin:5px"><i class="fa fa-floppy-o" aria-hidden="true"></i> @Resource.BtnSave</button>
                                <button class="btn btn-default" type="button" ng-click="cancel()"><i class="fa fa-ban" aria-hidden="true"></i> @Resource.BtnCancel</button>
                            </span>
                        </div>
                    </div>
                </form>

              
                <h2>Tags List</h2>
                <table style="overflow:hidden" class="table table-striped table-bordered dataTable no-footer dtr-inline" role="grid" aria-describedby="datatable-buttons_info">
                    <thead>
                        <tr role="row">
                            <th class="sorting_asc" tabindex="0" aria-controls="datatable-buttons" rowspan="1" colspan="1" style="width: 218px;" aria-sort="ascending" aria-label="Name: activate to sort column descending">Tag name</th>
                            <th class="sorting_asc" tabindex="0" aria-controls="datatable-buttons" rowspan="1" colspan="1" style="width: 218px;" aria-sort="ascending" aria-label="Name: activate to sort column descending">Project ID</th>
                            <th class="sorting" tabindex="0" aria-controls="datatable-buttons" rowspan="1" colspan="1" style="width: 92px;" aria-label="Actions: activate to sort column ascending">Actions</th>
                    </thead>
                    <tbody style="overflow:hidden">
                        <tr role="row" class="odd" href="" dir-paginate="tag in tags | itemsPerPage: 10" current-page="1" pagination-id="paginationTAG">


                            <td tabindex="0" class="sorting_1">{{tag.name}}</td>
                            <td tabindex="0" class="sorting_1">{{tag.Project_ID}}</td>
                            <td>
                                <div class="btn-group">
                                    <button class="btn btn-sm btn-default" type="button" data-placement="top" data-toggle="tooltip" data-original-title="Forward" aria-describedby="tooltip802045" ng-click="requirementDetails(requirement.Id)"><i class="fa fa-info-circle"></i></button>
                                    <button class="btn btn-sm btn-default" type="button" data-placement="top" data-toggle="tooltip" data-original-title="Print" ui-sref="requirementEdit({id:requirement.Id, projectId:id})"><i class="fa fa-pencil"></i></button>
                                    <button data-toggle="modal" data-target="#modalRequirementForm" class="btn btn-sm btn-default" type="button" data-placement="top" data-original-title="Trash" ng-click="reqSelected(requirement)"><i class="fa fa-trash-o"></i></button>
                                </div>
                            </td>
                        </tr>
                        <tr ng-if="filteredArray.length===0">
                            <td colspan="6">@Resource.NoDataFound</td>
                        </tr>
                    </tbody>
                </table>

            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-primary" data-dismiss="modal">Close</button>
            </div>
        </div>
    </div>
</div>
<div class="modal fade" id="loadingModalReport" tabindex="-1" role="dialog" aria-labelledby="loadingModalLabel" aria-hidden="true">
    <div class="modal-dialog modalCenter" role="document">
        <div class="modal-content">
            <div class="modal-body text-center">
                <div class="loader"></div>
                <div class="loader-txt">{{message}}</div>
            </div>
        </div>
    </div>
</div>
<div class="modal fade" id="modalSuplementalForm" tabindex="-1" role="dialog" aria-labelledby="myModalLabel"
     aria-hidden="true">
    <div class="modal-dialog" role="document" style="align-content:center;">
        <div class="modal-content modal-lg">
            <div class="modal-header">
                <h3>Disable Supplemental</h3>
            </div>
            <div class="modal-body" style="height:800px; width:100%; overflow-y:auto">
                <p style="overflow:hidden">Are you sure you want to disable the Supplemental: {{suplementalSelected.Title}}</p>

                <div class="subtitle modal-body">
                    <p>Part of these Test Procedures</p>
                </div>
                <div class="subtitle modal-body" ng-if="Procedures.length===0">
                    <p>No test procedures to display</p>
                </div>
                <div ng-if="Procedures.length>0">
                    <table style="width:100%; overflow:scroll;" class="table">
                        <thead>
                            <tr>
                                <th class="col-md-2">TP_Number</th>
                                <th class="col-md-7">Title</th>
                                <th class="col-lg-3">Priority</th>
                            </tr>
                        </thead>
                        <tbody>
                            <tr class="odd" ng-repeat="procedure in Procedures" ng-if="procedure.Status != 0">
                                <td class="col-md-2">{{procedure.tp_number}}</td>
                                <td class="col-md-8" style="text-overflow:ellipsis; overflow:hidden; white-space: nowrap;">{{procedure.Title}}</td>
                                <td class="col-md-2">{{procedure.Test_Priority}}</td>
                            </tr>
                        </tbody>
                    </table>
                </div>
                <div class="subtitle modal-body">
                    <p>Part of these Test Scenarios</p>
                </div>
                <div class="subtitle modal-body" ng-if="Scenarios.length===0">
                    <p>No data to display</p>
                </div>
                <div ng-if="Scenarios.length>=1">
                    <table style="width:100%; overflow:scroll;" class="table">
                        <thead>
                            <tr>
                                <th class="col-md-2">TS_Number</th>
                                <th class="col-md-7">Title</th>
                                <th class="col-lg-3">Priority</th>
                            </tr>
                        </thead>
                        <tbody>
                            <tr class="odd" ng-repeat="scenario in Scenarios" ng-if="scenario.Status != 0">
                                <td class="col-md-2">{{scenario.ts_number}}</td>
                                <td class="col-md-8" style="text-overflow:ellipsis; overflow:hidden; white-space: nowrap;" ng->{{scenario.Title}}</td>
                                <td class="col-md-2">{{scenario.Test_Priority}}</td>
                            </tr>
                        </tbody>
                    </table>
                </div>
            </div>
            <div class="modal-footer">
                <button class="btn btn-danger" data-dismiss="modal" ng-click="showModalToDeleteSuplemental(suplementalSelected)">Disable</button>
                <button type="button" class="btn btn-primary" data-dismiss="modal">Close</button>
            </div>
        </div>
    </div>
</div>
<div class="modal fade" id="Download" tabindex="-1" role="dialog" aria-labelledby="myModalLabel"
     aria-hidden="true">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h3>Download Report</h3>
            </div>
            <div class="modal-body" style="max-height:700px; width:100%;">
                <div class="message" ng-if="optionSelected=='Empty Report'">By selecting Empty Report, the report will not include any execution evidence</div>
                <div class="message" ng-if="optionSelected!='Empty Report'">By selecting last execution, the report will contain information on the last test execution</div>
                <form name="form" autocomplete="off" id="form" novalidate class="form-horizontal form-label-left" align="right" style="border:0">
                    <div class="form-group">
                        <label class="control-label col-md-2 col-sm-2 col-xs-1" for="Tester">
                            Select an option: <span class="required">*</span>
                        </label>
                        <div class="col-md-4 col-sm-4 col-lg-9">
                            <select name="priority" required class="form-control" ng-model="optionSelected"
                                    ng-options="option as option for option in Options2">
                                Choose An Option
                            </select>
                        </div>
                    </div>
                </form>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-danger" data-dismiss="modal" ng-click="downloadFileExcel()">Confirm</button>
                <button type="button" class="btn btn-primary" data-dismiss="modal">Close</button>
            </div>
        </div>
    </div>
</div>
