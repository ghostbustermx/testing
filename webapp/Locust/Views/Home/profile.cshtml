@Scripts.Render("~/bundles/AdjustUI")

@{
    Layout = null;
}

<div class="right_col" role="main">
    <div class="clearfix"></div>
    <div class="row">
        <div class="col-md-12 col-sm-12 col-xs-12">
            <div class="x_panel">
                <div class="x_title">

                    <div class="col-md-6 col-sm-12 col-xs-12">
                        <h2>Profile</h2>
                    </div>
                    <div class="clearfix"></div>
                </div>
                <div class="container">
                    <div class="x_content">
                        <div class="customContainer col-md-3 col-sm-3 col-xs-12 profile_left">
                            <div class="profile_img">
                                <div id="crop-avatar">

                                    <img class="img-responsive avatar-view" ng-src="{{info.PhotoUrl}}" alt="Avatar" title="Change the avatar">
                                </div>
                            </div>
                            <h3>{{info.FirstName}} {{info.LastName}}</h3>
                            <ul class="list-unstyled user_data">
                                <li>
                                    <i class="fa fa-user  user-profile-icon"></i> <strong>UserName:</strong> {{info.UserName}}
                                </li>
                                <li>
                                    <i class="fa fa-envelope  user-profile-icon"></i> <strong>Email:</strong>  {{info.Email}}
                                </li>
                                <li>

                                    <i class="fa fa-users  user-profile-icon"></i> <strong>Role:</strong>  {{info.Role}}
                                </li>
                                <li>
                                    <i class="fa   fa-industry  user-profile-icon"></i> <strong>Division:</strong> {{info.Division}}
                                </li>
                                <li>
                                    <i class="fa fa-briefcase  user-profile-icon"></i> <strong>Job Title:</strong> {{info.JobTitle}}
                                </li>
                                <li>
                                    <i class="fa fa-building  user-profile-icon"></i> <strong>Department:</strong> {{info.Department}}
                                </li>
                                <li>
                                    <i class="fa fa-thumbs-up user-profile-icon"></i> <strong>Alias:</strong> {{info.Alias}}
                                </li>
                                <li>
                                    <i class="fa fa-calendar  user-profile-icon"></i> <strong>Hire Date:</strong> {{info.HireDate}}
                                </li>
                            </ul>

                            <br>

