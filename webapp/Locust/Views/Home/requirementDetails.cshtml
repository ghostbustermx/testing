@Scripts.Render("~/bundles/AdjustUI")
@{
    Layout = null;
}

<div class="right_col" role="main">
    <nav aria-label="breadcrumb">
        <ol class="breadcrumb">
            <li class="breadcrumb-item"><a href="#">Home</a></li>
            <li class="breadcrumb-item" ng-click="projectDetails()"><a href="" ng-click="projectDetails()">{{Project_Id}}</a></li>
            <li class="breadcrumb-item active" aria-current="page">{{req_number}}</li>
        </ol>
    </nav>
    <div class="clearfix"></div>
    <div class="row">
        <div class="col-md-12 col-sm-12 col-xs-12">
            <div class="x_panel">
                <div class="x_title">
                    <h2>Requirement "{{name}}"<small> Details</small></h2>
                    <ul class="nav navbar-right panel_toolbox" id="demo">
                        <li>
                            <a class="collapse-link" data-toggle="collapse" data-target="#hi" id="collapse"><i class="fa fa-chevron-up"></i></a>
                        </li>
                    </ul>
                    <div class="clearfix"></div>
                </div>
                <script>
                    $(document).ready(function () {
                        $("#hi").on("hide.bs.collapse", function () {
                            $("#demo").html('<li> <a class="collapse-link" data-toggle="collapse" data-target="#hi" id="collapse"><i class="fa fa-chevron-down"></i></a></li>');
                        });
                        $("#hi").on("show.bs.collapse", function () {
                            $("#demo").html('<li> <a class="collapse-link" data-toggle="collapse" data-target="#hi" id="collapse"><i class="fa fa-chevron-up"></i></a></li>');
                        });
                        $('a[data-toggle="tab"]').on('show.bs.tab', function (e) {
                            localStorage.setItem('activeTabReq', $(e.target).attr('data-target'));
                        });

                        var activeTab = localStorage.getItem('activeTabReq');

                        if (activeTab) {
                            $('a[data-target="' + activeTab + '"]').tab('show');
                        }
                    });
                </script>
                @* Here start the contern *@
                <div class="x_content collapse in" id="hi" aria-expanded="true">
                    <div class="customContainer  col-lg-12 col-md-12 col-sm-12 col-xs-12">
                        <div class="col-lg-4 col-md-4 col-sm-12 col-xs-12  first-container">
                            <div>
                                <a class="customTitle">Req Number:</a>
                                <a class="message">{{req_number}}</a>
                            </div>
                            <div>
                                <a class="customTitle">Axosoft ID:</a>
                                <a class="message">{{axosoft_Task_Id}}</a>
                            </div>
                            <div>
                                <a class="customTitle">Developer Assigned:</a>
                                <a class="message">{{developerAssigned}}</a>
                            </div>
                            <div>
                                <a class="customTitle">Tester Assigned:</a>
                                <a class="message">{{testerAssigned}}</a>
                            </div>
                            <div>
                                <a class="customTitle">Status:</a>
                                <span class="label {{class}}">{{status}}</span>
                            </div>
                            <div>
                                <a class="customTitle">Attachments</a>
                                    <div class="list-group">
                                        <div ng-repeat="files in FilestoDisplay">
                                                <a style="cursor:pointer;" ng-click="DownloadFile(files.Id)"><b>{{files.Name}}</b></a>
                                        </div>
                                    </div>
                            </div>
                            <div class="input-group-btn" style="text-align:right">
                                <button type="button" class="btn btn-default btn-first-panel" aria-label="Change Log" ng-click="requirementChangelog()"><i class="fa fa-history" aria-hidden="true"></i> Change Log </button>
                            </div>
                        </div>              
                        <div class="InfoDetailsContainer col-lg-4 col-md-4 col-sm-12 col-xs-12">
                            <div>
                                <a class="customTitle">Description:</a>
                                <p class="message">{{description}}</p>
                            </div>
                        </div>
                        <div class="InfoDetailsContainer col-lg-4 col-md-4 col-sm-12 col-xs-12">
                            <div>
                                <a class="customTitle">Acceptance Criteria:</a>
                                <p class="message">{{acceptanceCriteria}}</p>
                            </div>
                          
                        </div>
                    </div>
                </div>


                <div class="x_content">
                    <div class="tab" role="tabpanel">
                        <ul class="nav nav-tabs" role="tablist">
                            <li role="presentation" class="active">
                                <a data-target="#tab_content1" id="home-tab" role="tab" data-toggle="tab" aria-expanded="true">Test Cases</a>
                            </li>
